<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.NhatKy" table="NHATKY">
		<id name="nkId" type="int">
			<column name="NKID" />
			<generator class="increment" />
		</id>
 		<property name="msnv" type="string" access="field"> 
 			<column name="MSNV" /> 
 		</property>
 		<property name="thoiGian" type="java.sql.Date" access="field"> 
 			<column name="thoiGian" /> 
 		</property>
 		<property name="hoatDong" type="string" access="field"> 
 			<column name="HOATDONG" /> 
 		</property>  
 		<property name="noiDung" type="string" access="field"> 
 			<column name="NOIDUNG" /> 
 		</property> 
	</class>
</hibernate-mapping>
